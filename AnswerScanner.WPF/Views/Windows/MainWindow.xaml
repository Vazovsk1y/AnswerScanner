                    </Image.Source>
                </Image>

                <Menu
                    Grid.Column="1"
                    Margin="5,0,0,0"
                    Background="Transparent"
                    FontSize="20">
                    <MenuItem Padding="5,10,5,10">
                        <MenuItem.Header>
                            <TextBlock
                                FontWeight="SemiBold"
                                Foreground="White"
                                Text="Файл" />
                        </MenuItem.Header>
                        <MenuItem
                            MinHeight="40"
                            Command="{Binding UploadQuestionnairesCommand}"
                            Header="Загрузить">
                            <MenuItem.Icon>
                                <Image Source="/Resources/Icons/upload.png" Stretch="Fill" />
                            </MenuItem.Icon>
                        </MenuItem>

                        <MenuItem MinHeight="40" Header="Экспортировать">
                            <MenuItem.Icon>
                                <Image Source="/Resources/Icons/export.png" Stretch="Fill" />
                            </MenuItem.Icon>
                        </MenuItem>
                    </MenuItem>
                </Menu>
                
                <Grid Grid.Column="2" MouseDown="OnMouseDown"/>

                <StackPanel Grid.Column="3" Orientation="Horizontal">
                    <StackPanel.Resources>
                        <Style TargetType="Button">
                            <Setter Property="Width" Value="48"/>
                            <Setter Property="Height" Value="50"/>
                            <Setter Property="BorderThickness" Value="0"/>
                            <Setter Property="Background" Value="Transparent"/>
                        </Style>
                    </StackPanel.Resources>
                    <Button
                        Click="MinimizeButton_Click"
                        ToolTip="Свернуть">
                        <Path
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Data="M0,0 L10,0"
                            Stroke="White"
                            StrokeThickness="2" />
                    </Button>

                    <Button
                        Click="MaximizeButton_Click"
                        ToolTip="Развернуть">
                        <Path
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Data="M0,0 L10,0 10,10 0,10 Z"
                            Fill="Transparent"
                            Stroke="White"
                            StrokeThickness="2" />
                    </Button>

                    <Button
                        Click="CloseButton_Click"
                        ToolTip="Закрыть">
                        <Path
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Data="M0,0 L10,10 M10,0 L0,10"
                            Stroke="Red"
                            StrokeThickness="2" />
                    </Button>
                </StackPanel>

            </Grid>

            <Grid Margin="0,5,0,0" Background="{StaticResource BackgroundBrush}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <ListBox
                    Margin="5,0,0,0"
                    Background="{StaticResource BackgroundBrush}"
                    BorderThickness="0"
                    ItemsSource="{Binding UploadedQuestionnaires}"
                    SelectedItem="{Binding SelectedQuestionnaire}"
                    SelectionMode="Single">
                    <ListBox.Resources>
                        <Style TargetType="{x:Type ListBoxItem}">
                            <Setter Property="Margin" Value="0,0,0,3" />
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="ListBoxItem">
                                        <Border
                                            Background="{TemplateBinding Background}"
                                            BorderBrush="{TemplateBinding BorderBrush}"
                                            BorderThickness="{TemplateBinding BorderThickness}"
                                            CornerRadius="3">
                                            <ContentPresenter />
                                        </Border>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="#d0d0d0" />
                                    <Setter Property="TextElement.Foreground" Value="White" />
                                </Trigger>
                                <Trigger Property="IsMouseOver" Value="False">
                                    <Setter Property="Background" Value="White" />
                                    <Setter Property="TextElement.Foreground" Value="Black" />
                                </Trigger>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Background" Value="#7d7d7d" />
                                    <Setter Property="TextElement.Foreground" Value="White" />
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </ListBox.Resources>

                    <ListBox.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Margin="15,15,15,10">
                                <StackPanel.Resources>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="FontSize" Value="16" />
                                    </Style>
                                </StackPanel.Resources>
                                <TextBlock
                                    FontWeight="SemiBold"
                                    Text="{Binding FilePath}"
                                    TextDecorations="Underline" />
                                <StackPanel Margin="0,5,0,0">
                                    <TextBlock Text="{Binding Questions.Count, StringFormat='Количество вопросов: {0}'}" />
                                    <TextBlock Margin="0,5,0,0" Text="{Binding Type.DisplayTitle, StringFormat='Тип опросника: {0}'}" />
                                </StackPanel>
                            </StackPanel>
                        </DataTemplate>
                    </ListBox.ItemTemplate>
                </ListBox>

                <TabControl
                    Grid.Column="1"
                    Margin="5,0,0,0"
                    Background="{StaticResource BackgroundBrush}"
                    BorderBrush="LightGray"
                    FontSize="18"
                    Visibility="{Binding SelectedQuestionnaire, Converter={StaticResource NullVisibilityConverter}}">
                    <TabItem Header="Список вопросов">
                        <DataGrid
                            Margin="0,2,0,0"
                            AutoGenerateColumns="False"
                            Background="{StaticResource BackgroundBrush}"
                            BorderBrush="LightGray"
                            CanUserAddRows="False"
                            CanUserDeleteRows="False"
                            ColumnWidth="*"
                            FontSize="16"
                            IsReadOnly="True"
                            ItemsSource="{Binding SelectedQuestionnaire.Questions}"
                            SelectionMode="Single">
                            <DataGrid.Resources>
                                <Style x:Key="BaseColumnHeaderStyle" TargetType="DataGridColumnHeader">
                                    <Setter Property="FontSize" Value="18" />
                                </Style>

                                <Style x:Key="AnswerCellValueStyle" TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                    <Setter Property="VerticalAlignment" Value="Center" />
                                    <Setter Property="FontWeight" Value="SemiBold" />
                                </Style>

                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridTextColumn
                                    MinWidth="40"
                                    MaxWidth="40"
                                    Binding="{Binding Number}"
                                    Header="№"
                                    HeaderStyle="{StaticResource BaseColumnHeaderStyle}" />
                                <DataGridTextColumn
                                    MinWidth="300"
                                    Binding="{Binding Text}"
                                    Header="Текст вопроса">
                                    <DataGridTextColumn.HeaderStyle>
                                        <Style BasedOn="{StaticResource BaseColumnHeaderStyle}" TargetType="DataGridColumnHeader">
                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                        </Style>
                                    </DataGridTextColumn.HeaderStyle>
                                </DataGridTextColumn>
                                <DataGridTextColumn
                                    MinWidth="100"
                                    MaxWidth="150"
                                    Binding="{Binding Answer.DisplayTitle}"
                                    ElementStyle="{StaticResource AnswerCellValueStyle}"
                                    Header="Ответ">
                                    <DataGridTextColumn.HeaderStyle>
                                        <Style BasedOn="{StaticResource BaseColumnHeaderStyle}" TargetType="DataGridColumnHeader">
                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                        </Style>
                                    </DataGridTextColumn.HeaderStyle>
                                </DataGridTextColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                    </TabItem>
                    <TabItem Header="Дополнительная информация">
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                            <ItemsControl BorderThickness="0" ItemsSource="{Binding SelectedQuestionnaire.AdditionalInformation}">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <Expander Header="{Binding Key}">
                                            <TextBlock
                                                Text="{Binding Value}"
                                                TextAlignment="Justify"
                                                TextWrapping="Wrap" />
                                        </Expander>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                        </ScrollViewer>
                    </TabItem>
                </TabControl>

            </Grid>
        </DockPanel>
    </Grid>

</Window>
